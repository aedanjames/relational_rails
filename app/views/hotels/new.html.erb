<%= form_with url: '/hotels', method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :five_stars %>
<%= form.text_field :five_stars %>
<%= form.label :year_founded %>
<%= form.text_field :year_founded %>
<%= form.submit "Create Hotel" %>
<% end%>
